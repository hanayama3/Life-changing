<div class="complete-box pt-5">
<%= form_with(model: @user,url: complete_user_path, local: true) do |f|%>
<h4><%= @user.name %></h4>
<p class="pt-3">習慣</p>
<div class="form-box pt-3">
 <div class="form-item">
<%= f.collection_check_boxes :habit_ids, @user.habits,:id,:task,checked: false do |b| %>
<%= b.label do %>
<%= b.check_box %>
<%= b.text %>
<% end %>
<% end %>
</div>
<div class="form-item">
 <%= f.label :content, class: "label"%>
 <%= f.text_area :content, placeholder: "空でもOK", size: "40x7"%>
 <%= f.label :private, class: "label"%>
 <%= f.check_box :private %>
</div>
<%= f.submit "送信",class: "btn btn-primary m-5"%>
<% end %>
</div>
