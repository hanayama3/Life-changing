<div class="show-page">
<%= render 'user_show' %>
<p><%= link_to "習慣一覧", @user, class: "pl-4"%></p>
 
<div class="relation-box">
<h5><%= @title %></h5>
 </div>
<table class="table table-striped">
  <tr><th>名前</th><th>Lv.</th><th>登録日</th></tr>
   <% @users.each do |user| %>
  <tr><td> <%= link_to user.name, "/users/#{user.id}" %></td>
  <td><%= user.level %></td>
   <td><%= user.created_at.strftime("%y/%m/%d") %></td></tr>
  <% end %>
  </table>
 </div>
